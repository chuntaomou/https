                h3 Filter Results
              .module-canvas
                #job-detail-filter.job-detail-filter.sub-section
                  
                    fieldset.filter-criteria
                      .field.f-search-criteria
                        label(for='filter-search-criteria') Keywords
                        input#filter-search-criteria(type='text', name='s_Keywords', value='', placeholder='Job Keywords')
                      .filter.inactive
                        label(for='s_ContractType') Product Type
                        select#s_ContractType.filterSelect(name='s_ContractType')
                          option(value='9', selected='selected') - Choose - 
                          option(value='1') Product
                          option(value='2') Product Group
                          option(value='3') Product Class
                          option(value='4') Product Family
                          option(value='5') VAT Code
                          option(value='6') Product Brand
                          option(value='7') Product Sub Brand
                          option(value='8') Product Price Tier
                    fieldset.filter-submit
                      .action
                        button.update-filters(style='background-color:#00a200;') Update Results
                        button.clear-filters Clear Search
                    input#jobsearch-pagesize(type='hidden', name='pageSize')
                  form#filter-form-surface(action='/umbraco/surface/JobFilter/UpdateFilters')
                    input#surface_s_ContractType(type='hidden', name='s_ContractType')
                    input#surface_s_WorkingHours(type='hidden', name='s_WorkingHours')
                    input#surface_s_Region(type='hidden', name='s_Region')
                    input#surface_s_Category(type='hidden', name='s_Category')
