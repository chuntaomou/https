        .section-canvas
          #background-image.row.row--background-image(style='height: 360px;')
            .m-banner-search(style='margin-top:140px;')
              .module-canvas
                .field.f-search
                  input#search-criteria.search-box(type='text', name='s_Keywords', value='', placeholder='Search for a role ...')
                .field.f-submit.desktop
                  button#search-products-button(style='width:166px; height:58px; background-color:#00a200;') Search
                .field.f-filter-1
                  select#s_Region.filterSelect1(name='s_Region')
                    option#1 Search by name
                    option#2(selected='selected') Search by code
                .field.f-filter-2
                  select#s_Category.filterSelect1(name='s_Category')
                    option#1(selected='selected') Product
                    option#2 Product Group
                    option#3 Product Class
                    option#4 Product Family
                    option#5 VAT Code
                    option#6 Product Brand
                    option#7 Product Sub Brand
                    option#8 Product Price Tier
                    option#9 Any
